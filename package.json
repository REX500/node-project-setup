{
  "name": "node",
  "version": "1.0.0",
  "description": "Basic node structure using express",
  "main": "index.js",
  "scripts": {
    "dev": " NODE_ENV=development nodemon ./bin/billy-api-server",
    "dev:win": "SET NODE_ENV=development&& nodemon ./bin/billy-api-server",
    "migration:create": "sequelize migration:create --config ./api/lib/db/config/config.js --name",
    "migration:undo": "sequelize db:migrate:undo --config ./api/lib/db/config/config.js",
    "migration:migrate": "sequelize db:migrate --config ./api/lib/db/config/config.js",
    "migration:seed": "sequelize db:seed:all --config ./api/lib/db/config/config.js"
  },
